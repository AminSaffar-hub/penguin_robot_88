        <param name="image_topic" value="/camera/rgb/image_raw" />
        <param name="output_image_topic" value="/detector_ros/labeled" />
        <param name="point_cloud_topic" value="/camera/depth/points" />
        <param name="global_frame" value="odom" /> 
        <param name="camera_link" value="/kinect_link" /> 
        <param name="confidence_thresh" value="0.9" /> 
    </node>-->
    
    
</launch>