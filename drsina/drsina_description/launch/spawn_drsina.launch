<?xml version="1.0" encoding="UTF-8"?>
<launch>
    <arg name="world_file"  default="$(find drsina_description)/worlds/hospital_room.world"/>   
    <arg name="paused" default="false"/>
    <arg name="use_sim_time" default="true"/>
    <arg name="gui" default="true"/>
    <arg name="headless" default="false"/>
    <arg name="debug" default="false"/>

    <include file="$(find gazebo_ros)/launch/empty_world.launch">
        <arg name="world_name" value="$(arg world_file)" />
        <arg name="paused" value="$(arg paused)"/>
        <arg name="use_sim_time" value="$(arg use_sim_time)"/>
        <arg name="gui" value="$(arg gui)"/>
        <arg name="headless" value="$(arg headless)"/>
        <arg name="debug" value="$(arg debug)"/>
    </include>
    
    <include file="$(find drsina_description)/launch/spawn_urdf.launch">
        <arg name="x" value="0.0"/>
        <arg name="y" value="0.0"/>
        <arg name="z" value="0.2"/>
        <arg name="urdf_robot_file" value="$(find drsina_description)/urdf/drsina.urdf"/>
        <arg name="robot_name" value="drsina"/>
    </include>

    <include file="$(find drsina_description)/launch/drsina_control.launch"/>

    <!--<node name="rviz" pkg="rviz" type="rviz" args="-d $(find drsina_description)/rviz_config/drsina_simple.rviz)"/>-->
    <!--<node name="image_converter" pkg="ros_darknet" type="darknet_ros.py"/>-->

    <!--<include file="$(find freenect_launch)/launch/freenect.launch"/>-->
