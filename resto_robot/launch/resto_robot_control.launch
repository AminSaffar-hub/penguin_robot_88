<?xml version="1.0" encoding="UTF-8"?>
<launch>
    <rosparam file="$(find resto_robot)/config/resto_robot_control.yaml" command="load"/>
    <rosparam file="$(find resto_robot)/config/joint_states.yaml" command="load"/>
    <!--rosparam file="$(find drsina_description)/config/gazebo_ros_param.yaml" command="load"/-->
    <!--load the controllers-->
    <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
    output="screen" ns="/resto_robot" args="move_base_controller joint_state_controller "/>
    <!--convert joint states to TF transforms for rviz ,etc-->
    <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" respawn="false" output="screen">
        <param name="publish_frequency" type="double" value="30.0"/>
        <remap from="/joint_states" to ="/resto_robot/joint_states"/>
    </node>
</launch>