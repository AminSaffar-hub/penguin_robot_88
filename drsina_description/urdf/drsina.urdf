    <link name="base_link">
        <visual>
            <origin rpy="0.0 0 0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="package://drsina_description/models/meshes/chassis.dae"/>
            </geometry>
            <material name="grey"/>
        </visual>
        <collision>
            <origin rpy="0.0 0 0" xyz="0 0 0"/>
